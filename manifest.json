{
  "manifest_version": 2,
  "name": "Instagram Stories Volume Changer",
  "description": "Change volume from Instagram Stories.",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/stories/*/*/", "https://www.instagram.com/*"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "128": "assets/icon128.png"
  }
}