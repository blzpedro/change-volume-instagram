{
    "manifest_version": 1,
    "name": "Change volume",
    "description": "Alterar o volume dos stories do Instagram.",
    "version": "1",
    "content_scripts": [
      {
        "matches": ["https://instagram.com/"],
        "js": ["index.js"]
      }
    ],
    "icons": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "128": "assets/icon128.png"
    }
  }